{
  "manifest_version": 2,
  "name": "Bennett's Hockey Extention",
  "version": "1.0.0",
  "web_accessible_resources": [
    "hockey/*.jpg"
  ],
  "icons": { "128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png"
    },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["Hockey.js"]
  }]


}
